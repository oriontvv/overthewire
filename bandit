import os
import pwd

for root, dirs, fnames in os.walk('/'):
    for fname in fnames:
        fullpath = os.path.join(root, fname)
        size = os.path.getsize(fullpath)
        if size != 33:
            continue

        uid = os.stat(fullpath).st_uid
        if pwd.getpwuid(uid).pw_name != 'bandit7':
            continue

        print(fullpath)
        with open(fullpath) as f:
            print(f.read())

